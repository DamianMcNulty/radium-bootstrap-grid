    "babel-core": "6.18.2",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": ">=0.1.0",
    "eslint-plugin-react": ">=3.5.1",
    "react-transform-catch-errors": ">=1.0.0",
    "react-transform-hmr": ">=1.0.1",
    "rimraf": ">=2.4.3"
  },
  "peerDependencies": {
    "radium": ">=0.18.1",
    "react": ">=15.4.2"
